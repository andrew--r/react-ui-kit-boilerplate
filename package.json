{
  "name": "react-ui-kit-boilerplate",
  "version": "1.0.0",
  "main": "lib/index.js",
  "author": "Andrew Romanov <me@andrew-r.ru>",
  "license": "MIT",
  "scripts": {
    "start": "styleguidist server",
    "build": "yarn build:bundle && yarn build:styleguide",
    "build:bundle": "rm -rf lib && ./node_modules/.bin/cross-env NODE_ENV=production ./node_modules/.bin/webpack --progress --colors",
    "build:styleguide": "styleguidist build",
    "lint": "./node_modules/.bin/eslint --ext .js --ext .jsx ./source",
    "plop": "plop"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^3.1.3",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.5.0",
    "plop": "^1.5.0",
    "pre-commit": "^1.1.3",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-styleguidist": "^4.1.0",
    "webpack": "^1.13.3",
    "webpack-config": "^6.2.1",
    "webpack-dashboard": "^0.2.0"
  },
  "peerDependencies": {
    "react": "^15.3.2"
  }
}
